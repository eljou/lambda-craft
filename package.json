	"version": "0.1.0",
	"description": "Modern, and practical utility library for FP in TypeScript",
	"type": "module",
	"main": "src/index.mjs",
	"typings": "src/index.d.ts",
	"files": [
		"src/**/*.mjs",
		"src/**/*.d.ts"
	],
	"bugs": {
		"url": "https://github.com/eljou/lambda-craft/issues"
	},
	"homepage": "https://github.com/eljou/lambda-craft#readme",
